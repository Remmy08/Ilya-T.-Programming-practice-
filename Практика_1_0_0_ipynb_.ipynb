        "class BankAccount:\n",
        "    def __init__(self, account_holder):\n",
        "        self.account_holder = account_holder\n",
        "        self.balance = 0\n",
        "\n",
        "    def deposit(self, amount):\n",
        "        self.balance += amount\n",
        "        print(f\"Вы положили {amount} на счет. Баланс: {self.balance}\")\n",
        "\n",
        "    def withdraw(self, amount):\n",
        "        if amount > self.balance:\n",
        "            print(\"Недостаточно денег на счете!\")\n",
        "        else:\n",
        "            self.balance -= amount\n",
        "            print(f\"Вы сняли {amount} со счета. Баланс: {self.balance}\")\n",
        "\n",
        "    def get_balance(self):\n",
        "        print(f\"Ваш баланс: {self.balance}\")\n",
        "\n",
        "\n",
        "\n",
        "print(\"========== БАНКЪ ==========\")\n",
